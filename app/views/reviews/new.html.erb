<h2>Write a review for <%= @product.name %></h2>
<%= form_for @review, :url => products_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br><br>
  <%= f.label :cost %>
  <%= f.text_field :cost %><br><br>
  <%= f.label :country_of_origin %>
  <%= f.text_field :country_of_origin %><br><br>
  <%= f.submit %>
<% end %>
